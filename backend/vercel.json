{
    "version": 2,
    "builds": [{
        "src": "backend_netropolis/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [{
        "src": "/static/(.*)",
        "dest": "/static/$1"
    }, {
        "src": "/(.*)",
        "dest": "backend_netropolis/wsgi.py"
    }]
}